<table>
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Age</th>
    </tr>
    <% students.forEach((s) => { %>
        <tr>
            <td><%= s.student_id %></td>
            <td><%= s.student_name %></td>
            <td><%= s.age %></td>
            <td><a href="/students/delete/<%= s.student_id%>">Delete</a></td>
        </tr>
    <% }) %>
</table>
